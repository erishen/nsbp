{
  "name": "nsbp",
  "version": "1.0.0",
  "description": "node react ssr by webpack",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:**",
    "dev:start": "nodemon --watch build --exec node ./scripts/start.js",
    "dev:build:server": "webpack --config webpack.server.js --mode development --watch",
    "dev:build:client": "webpack --config webpack.client.js --mode development --watch",
    "build": "npm run clean && npm-run-all --parallel build:**",
    "build:server": "webpack --config webpack.server.js --mode production",
    "build:client": "webpack --config webpack.client.js --mode production",
    "start": "node ./scripts/start.js",
    "clean": "rimraf build && rimraf public/js && rimraf public/client.*",
    "format": "prettier --write **/*.{js,css,less,scss,ts,tsx}",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "ssr",
    "webpack",
    "less",
    "scss",
    "sass",
    "styled-components"
  ],
  "author": "Erishen Sun",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "html-minifier": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^5.0.1",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@types/express": "^4.17.11",
    "@types/html-minifier": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/react-motion": "0.0.29",
    "@types/react-router-dom": "^5.1.7",
    "@types/serialize-javascript": "^5.0.0",
    "@types/styled-components": "^5.1.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "less": "^4.1.1",
    "less-loader": "^8.1.1",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.4",
    "typescript-plugin-styled-components": "^1.5.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webp-loader": "^0.6.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^3.0.0"
  }
}
