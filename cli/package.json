{
  "name": "nsbp-cli",
  "version": "0.2.8",
  "description": "CLI tool for creating NSBP (Node React SSR by Webpack) projects",
  "main": "index.js",
  "files": [
    "bin/**/*",
    "templates/**/*",
    "README.md"
  ],
  "bin": {
    "nsbp": "./bin/nsbp.js"
  },
  "scripts": {
    "start": "node ./bin/nsbp.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sync-template": "node ./scripts/sync-template.js",
    "update": "npm run sync-template",
    "update-changelog": "node ./scripts/update-changelog.js",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "keywords": [
    "nsbp",
    "cli",
    "react",
    "ssr",
    "webpack",
    "scaffold",
    "generator"
  ],
  "author": "Erishen Sun",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/erishen/nsbp"
  },
  "bugs": {
    "url": "https://github.com/erishen/nsbp/issues"
  },
  "homepage": "https://github.com/erishen/nsbp#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "conventional-changelog": "^7.1.1",
    "conventional-changelog-angular": "^8.1.0",
    "conventional-changelog-cli": "2.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
